<a class="box-title" href="{{ path('home') }}">
    <img class="rounded-circle" src="{{ contact.logo }}" alt="" width="100" height="100">
    <h1>{{ contact.name }}</h1>
</a>

<div class="box-address">
    <p>{{ contact.ligne1 }}</p>
    <p>{{ contact.cp ~ ' - ' ~  contact.ville ~ ' - ' ~ contact.pays }}</p>
    <p>{{ 'Email : ' ~ contact.email ~ ' - ' ~ 'Tel : ' ~ contact.numTel }}</p>
</div>

